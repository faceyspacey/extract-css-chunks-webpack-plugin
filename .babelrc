{
	"presets": [
		[
			"env",
			{
				"useBuiltIns": true,
				"targets": {
					"node": "6.11.5",
					"browsers": "cover 99.5%"
				},
				"exclude": [
					"transform-async-to-generator",
					"transform-regenerator"
				]
			}
		]
	],
	"plugins": [
		[
			"transform-object-rest-spread",
			{
				"useBuiltIns": true
			}
		]
	],
	"env": {
		"test": {
			"presets": [
				"env"
			],
			"plugins": [
				"transform-object-rest-spread"
			]
		}
	}
}
